@prefix modgui: <http://moddevices.com/ns/modgui#> .
@prefix lv2:    <http://lv2plug.in/ns/lv2core#> .
@prefix ui:     <http://lv2plug.in/ns/extensions/ui#> .

<http://moddevices.com/plugins/mod-devel/Gain2x2>
    modgui:gui [
        modgui:resourcesDirectory <modgui> ;
        modgui:iconTemplate <modgui/icon-gain-2x2.html> ;
        modgui:stylesheet <modgui/stylesheet-gain-2x2.css> ;
        modgui:screenshot <modgui/screenshot-gain-2x2.png> ;
        modgui:thumbnail <modgui/thumbnail-gain-2x2.png> ;
        modgui:brand "MOD" ;
        modgui:label "Stereo Gain" ;
        modgui:model "boxy" ;
        modgui:panel "1-knob" ;
        modgui:color "gray" ;
        modgui:port [
            lv2:index 0 ;
            lv2:symbol "Gain" ;
            lv2:name "Gain" ;
        ] ;
    ] .