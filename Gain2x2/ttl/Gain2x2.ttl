@prefix lv2:  <http://lv2plug.in/ns/lv2core#>.
@prefix doap: <http://usefulinc.com/ns/doap#>.
@prefix epp: <http://lv2plug.in/ns/ext/port-props/#>.
@prefix foaf: <http://xmlns.com/foaf/0.1/>.
@prefix units: <http://lv2plug.in/ns/extension/units#> .
@prefix mod: <http://portalmod.com/ns/modgui#>.

<http://portalmod.com/plugins/mod-devel/Gain2x2>
a lv2:Plugin, lv2:UtilityPlugin;

doap:name "Gain 2x2";

doap:developer [
    foaf:name "Andre";
    foaf:homepage <>;
    foaf:mbox <mailto:andre_coutinho@rocketmail.com>;
    ];

doap:maintainer [
    foaf:name "MOD Team";
    foaf:homepage <http://portalmod.com>;
    foaf:mbox <mailto:devel@portalmod.com>;
    ];

doap:license <GPL>;
lv2:port 
[
    a lv2:AudioPort, lv2:InputPort;
    lv2:index 0;
    lv2:symbol "In_1";
    lv2:name "In 1";
],
[
    a lv2:AudioPort, lv2:InputPort;
    lv2:index 1;
    lv2:symbol "In_2";
    lv2:name "In 2";
],
[
    a lv2:AudioPort, lv2:OutputPort;
    lv2:index 2;
    lv2:symbol "Out1";
    lv2:name "Out 1";
],
[
    a lv2:AudioPort, lv2:OutputPort;
    lv2:index 3;
    lv2:symbol "Out2";
    lv2:name "Out 2";
],
[
    a lv2:ControlPort, lv2:InputPort;
    lv2:index 4;
    lv2:symbol "Gain";
    lv2:name "Gain";
    units:unit units:db ;
    lv2:default 3;
    lv2:minimum -40;
    lv2:maximum 40;
];
    mod:gui [
        a mod:Gui;
        mod:resourcesDirectory <modgui>;
        mod:iconTemplate <modgui/icon-gain-2x2.html>;
        mod:templateData <modgui/data-gain-2x2.json>;
        mod:screenshot <modgui/screenshot-gain-2x2.png>;
        mod:thumbnail <modgui/thumb-gain-2x2.png>;
    ] .
