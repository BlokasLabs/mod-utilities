@prefix lv2:  <http://lv2plug.in/ns/lv2core#>.
@prefix doap: <http://usefulinc.com/ns/doap#>.
@prefix epp: <http://lv2plug.in/ns/ext/port-props#>.
@prefix foaf: <http://xmlns.com/foaf/0.1/>.
@prefix mod: <http://moddevices.com/ns/modgui#>.
@prefix rdf:  <http://www.w3.org/1999/02/22-rdf-syntax-ns#>.
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#>.

<http://moddevices.com/plugins/mod-devel/Pot>
a lv2:Plugin, lv2:UtilityPlugin;

doap:name "Pot";

doap:developer [
    foaf:name "Jesse Verhage";
    foaf:homepage <>;
    foaf:mbox <mailto:jesse@moddevices.com>;
    ];

doap:maintainer [
    foaf:name "MOD Team";
    foaf:homepage <http://moddevices.com>;
    foaf:mbox <mailto:devel@moddevices.com>;
    ];

rdfs:comment """
Plugin that works like a regular volume potentiometer, switchable between different curves.
""";

doap:license <GPL>;

lv2:minorVersion 1;
lv2:microVersion 1;

lv2:port 
[
    a lv2:AudioPort, lv2:InputPort;
    lv2:index 0;
    lv2:symbol "In";
    lv2:name "In";
],
[
    a lv2:AudioPort, lv2:OutputPort;
    lv2:index 1;
    lv2:symbol "Out";
    lv2:name "Out";
],
[
    a lv2:ControlPort, lv2:InputPort;
    lv2:index 2;
    lv2:symbol "Level";
    lv2:name "Level";
    lv2:shortname "Lvl";
    lv2:default 1;
    lv2:minimum 0;
    lv2:maximum 1;
].
