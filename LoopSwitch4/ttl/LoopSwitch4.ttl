@prefix lv2:  <http://lv2plug.in/ns/lv2core#>.
@prefix doap: <http://usefulinc.com/ns/doap#>.
@prefix epp: <http://lv2plug.in/ns/ext/port-props#>.
@prefix foaf: <http://xmlns.com/foaf/0.1/>.
@prefix mod: <http://moddevices.com/ns/modgui#>.
@prefix rdf:  <http://www.w3.org/1999/02/22-rdf-syntax-ns#>.
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#>.

<http://moddevices.com/plugins/mod-devel/LoopSwitch4>
a lv2:Plugin, lv2:UtilityPlugin;

doap:name "LoopSwitch";

doap:developer [
    foaf:name "Jesse Verhage";
    foaf:homepage <>;
    foaf:mbox <mailto:jesse@moddevices.com>;
    ];

doap:maintainer [
    foaf:name "MOD Team";
    foaf:homepage <http://moddevices.com>;
    foaf:mbox <mailto:devel@moddevices.com>;
    ];

rdfs:comment """
This loop switcher enables/disables loops depending on the state of the toggle.
""";

doap:license <GPL>;

lv2:minorVersion 1;
lv2:microVersion 1;

lv2:port 
[
    a lv2:AudioPort, lv2:InputPort;
    lv2:index 0;
    lv2:symbol "In";
    lv2:name "In";
],
[
    a lv2:AudioPort, lv2:InputPort;
    lv2:index 1;
    lv2:symbol "Ret1";
    lv2:name "Return 1";
],
[
    a lv2:AudioPort, lv2:InputPort;
    lv2:index 2;
    lv2:symbol "Ret2";
    lv2:name "Return 2";
],
[
    a lv2:AudioPort, lv2:InputPort;
    lv2:index 3;
    lv2:symbol "Ret3";
    lv2:name "Return 3";
],
[
    a lv2:AudioPort, lv2:InputPort;
    lv2:index 4;
    lv2:symbol "Ret4";
    lv2:name "Return 4";
],
[
    a lv2:AudioPort, lv2:OutputPort;
    lv2:index 5;
    lv2:symbol "Out";
    lv2:name "Out";
],
[
    a lv2:AudioPort, lv2:OutputPort;
    lv2:index 6;
    lv2:symbol "Snd1";
    lv2:name "Send 1";
],
[
    a lv2:AudioPort, lv2:OutputPort;
    lv2:index 7;
    lv2:symbol "Snd2";
    lv2:name "Send 2";
],
[
    a lv2:AudioPort, lv2:OutputPort;
    lv2:index 8;
    lv2:symbol "Snd3";
    lv2:name "Send 3";
],
[
    a lv2:AudioPort, lv2:OutputPort;
    lv2:index 9;
    lv2:symbol "Snd4";
    lv2:name "Send 4";
],
[
    a lv2:ControlPort, lv2:InputPort;
    lv2:index 10;
    lv2:symbol "Loop1";
    lv2:name "Loop One";
    lv2:portProperty lv2:toggled;
    lv2:default 0;
    lv2:minimum 0;
    lv2:maximum 1;
],
[
    a lv2:ControlPort, lv2:InputPort;
    lv2:index 11;
    lv2:symbol "Loop2";
    lv2:name "Loop Two";
    lv2:portProperty lv2:toggled;
    lv2:default 0;
    lv2:minimum 0;
    lv2:maximum 1;
],
[
    a lv2:ControlPort, lv2:InputPort;
    lv2:index 12;
    lv2:symbol "Loop3";
    lv2:name "Loop Three";
    lv2:portProperty lv2:toggled;
    lv2:default 0;
    lv2:minimum 0;
    lv2:maximum 1;
],
[
    a lv2:ControlPort, lv2:InputPort;
    lv2:index 13;
    lv2:symbol "Loop4";
    lv2:name "Loop Four";
    lv2:portProperty lv2:toggled;
    lv2:default 0;
    lv2:minimum 0;
    lv2:maximum 1;
];
