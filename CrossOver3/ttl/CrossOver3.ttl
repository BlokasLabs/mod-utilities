@prefix lv2:  <http://lv2plug.in/ns/lv2core#>.
@prefix doap: <http://usefulinc.com/ns/doap#>.
@prefix epp: <http://lv2plug.in/ns/ext/port-props#>.
@prefix pprops: <http://lv2plug.in/ns/ext/port-props#> .
@prefix foaf: <http://xmlns.com/foaf/0.1/>.
@prefix units: <http://lv2plug.in/ns/extensions/units#> .
@prefix rdf:  <http://www.w3.org/1999/02/22-rdf-syntax-ns#>.
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#>.
@prefix mod: <http://moddevices.com/ns/modgui#>.

<http://moddevices.com/plugins/mod-devel/CrossOver3>
a lv2:Plugin, lv2:FilterPlugin;

doap:name "CrossOver 3";

doap:developer [
    foaf:name "Andre";
    foaf:homepage <>;
    foaf:mbox <mailto:andre_coutinho@rocketmail.com>;
    ];

doap:maintainer [
    foaf:name "MOD Team";
    foaf:homepage <http://moddevices.com>;
    foaf:mbox <mailto:devel@moddevices.com>;
    ];

doap:license <GPL>;

rdfs:comment """
This plugin receives an input signal and outputs 3 filtered signals. First one is filtered with a low pass filter (LPF), the second with a band pass filter (BPF) and the third with a high pass filter (HPF), cutoff frequency used on LPF and as the lower frequency from BPF is controlled by "Freq 1" and cutoff frequency used as the higher frequency on BPF and in HPF is controlled by "Freq 2". "Order" indicates filter's orders (or how fast frequencies above (in HPF) or below (in LPF) the cutoff frequency will decay. Higher the order, faster the decay) ."Gain 1", "Gain 2" and "Gain 3" controls the gains of all outputs respectively.


""";

lv2:port
[
    a lv2:AudioPort, lv2:InputPort;
    lv2:index 0;
    lv2:symbol "In";
    lv2:name "In";
],
[
    a lv2:AudioPort, lv2:OutputPort;
    lv2:index 1;
    lv2:symbol "Out1";
    lv2:name "Out 1";
],
[
    a lv2:AudioPort, lv2:OutputPort;
    lv2:index 2;
    lv2:symbol "Out2";
    lv2:name "Out 2";
],
[
    a lv2:AudioPort, lv2:OutputPort;
    lv2:index 3;
    lv2:symbol "Out3";
    lv2:name "Out 3";
],
[
    a lv2:ControlPort, lv2:InputPort;
    lv2:index 4;
    lv2:symbol "Freq_1";
    lv2:name "Freq 1";
    lv2:portProperty epp:logarithmic;
    lv2:portProperty pprops:logarithmic ;
    units:unit units:hz ;
    lv2:default 100;
    lv2:minimum 20;
    lv2:maximum 20000;
],
[
    a lv2:ControlPort, lv2:InputPort;
    lv2:index 5;
    lv2:symbol "Freq_2";
    lv2:name "Freq 2";
    lv2:portProperty epp:logarithmic;
    lv2:portProperty pprops:logarithmic ;
    units:unit units:hz ;
    lv2:default 1000;
    lv2:minimum 20;
    lv2:maximum 20000;
],
[
    a lv2:ControlPort, lv2:InputPort;
    lv2:index 6;
    lv2:symbol "Gain_1";
    lv2:name "Gain 1";
    units:unit units:db ;
    lv2:default 3;
    lv2:minimum -40;
    lv2:maximum 40;
],
[
    a lv2:ControlPort, lv2:InputPort;
    lv2:index 7;
    lv2:symbol "Gain_2";
    lv2:name "Gain 2";
    units:unit units:db ;
    lv2:default 3;
    lv2:minimum -40;
    lv2:maximum 40;
],
[
    a lv2:ControlPort, lv2:InputPort;
    lv2:index 8;
    lv2:symbol "Gain_3";
    lv2:name "Gain 3";
    units:unit units:db ;
    lv2:default 3;
    lv2:minimum -40;
    lv2:maximum 40;
],
[
    a lv2:ControlPort, lv2:InputPort;
    lv2:index 9;
    lv2:symbol "Order";
    lv2:name "Order";
    lv2:portProperty lv2:integer;
        lv2:portProperty lv2:enumeration ;
        lv2:scalePoint [rdfs:label "1"; rdf:value 0];
        lv2:scalePoint [rdfs:label "2"; rdf:value 1];
        lv2:scalePoint [rdfs:label "3"; rdf:value 2];
    lv2:default 0;
    lv2:minimum 0;
    lv2:maximum 2;
];

mod:gui [
    mod:resourcesDirectory <modgui>;
    mod:iconTemplate <modgui/icon-crossover-3.html>;
    mod:stylesheet <modgui/stylesheet-crossover-3.css>;
    mod:templateData <modgui/data-crossover-3.json>;
    mod:screenshot <modgui/screenshot-crossover-3.png>;
    mod:thumbnail <modgui/thumb-crossover-3.png>;
];

lv2:minorVersion 1;
lv2:microVersion 0 .
